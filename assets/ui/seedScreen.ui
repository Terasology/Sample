{
    "type": "SeedScreen",
    "contents": {
        "type": "relativeLayout",
        "contents": [
            {
                "type": "UIBox",
                "layoutInfo": {
                    "width": 250,
                    "height": 130,
                    "position-horizontal-center": {},
                    "position-vertical-center": {}
                },
                "content": {
                    "type": "relativeLayout",
                    "contents": [
                        {
                            "type": "UILabel",
                            "id": "seedLabel",
                            "text": "Seed",
                            "layoutInfo": {
                                "use-content-width": true,
                                "position-horizontal-center": {},
                                "height": 30
                            }
                        },
                        {
                            "type": "UIText",
                            "text": "",
                            "id": "seedText",
                            "layoutInfo": {
                                "position-top": {
                                    "widget": "seedLabel",
                                    "target": "BOTTOM"
                                },
                                "position-bottom": {
                                    "widget": "seedUpdateButton",
                                    "target": "TOP"
                                }
                            },
                            "multiline": true,
                            "readOnly": true
                        },
                        {
                            "type": "UIButton",
                            "id": "seedUpdateButton",
                            "text": "Update Seed",
                            "layoutInfo": {
                                "use-content-height": true,
                                "position-bottom": {}
                            }
                        }
                    ]
                }
            }
        ]
    }
}